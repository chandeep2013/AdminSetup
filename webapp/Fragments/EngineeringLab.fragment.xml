								<html:div class= "cols-12 padding-20">
								<tree:TreeTable id="TreeTable" class="catTable" rows="{path:'/catalog/clothing', parameters: {arrayNames:['categories']}}" selectionMode="MultiToggle"
									enableSelectAll="false" ariaLabelledBy="title">
									<tree:columns>
										<tree:Column width="17rem">
											<Label text="Categories"/>
											<tree:template>
												<Text text="{name}" wrapping="false"/>
											</tree:template>
										</tree:Column>
										<tree:Column>
											<Label text="Start Date"/>
											<tree:template>
												<DatePicker class="tableData" value="{startDate}" visible="{= !!${startDate}}"/>
											</tree:template>
										</tree:Column>
										<tree:Column>
											<Label text="End Date"/>
											<tree:template>
												<DatePicker class="tableData" value="{endDate}" visible="{= !!${endDate}}"/>
											</tree:template>
										</tree:Column>
										<tree:Column>
											<Label text="Quantity"/>
											<tree:template>
												<Input value="{Quantity}" visible="{= !!${Quantity}}"/>
											</tree:template>
										</tree:Column>
										<tree:Column>
											<Label text="Rate"/>
											<tree:template>
												<Input value="{Rate}" visible="{= !!${Rate}}"/>
											</tree:template>
										</tree:Column>
										<tree:Column>
											<Label text="Unit"/>
											<tree:template>
												<Input value="{Unit}" visible="{= !!${Unit}}"/>
											</tree:template>
										</tree:Column>
										<tree:Column>
											<Label text="Charge"/>
											<tree:template>
												<Input value="{Charge}" visible="{= !!${Charge}}"/>
											</tree:template>
										</tree:Column>
									</tree:columns>
								</tree:TreeTable>
								</html:div>
